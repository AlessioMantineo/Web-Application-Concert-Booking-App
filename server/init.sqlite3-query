-- Update seat statuses for User 3 (normal)
UPDATE Seat SET status = 'occupied' WHERE seat_id IN (9, 10);  -- Concert 2
UPDATE Seat SET status = 'occupied' WHERE seat_id IN (51, 52);  -- Concert 4